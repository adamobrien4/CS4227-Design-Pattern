  <children>
    <AnchorPane fx:id="FXsignupPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="578.0" prefWidth="770.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="FXsignupButton" layoutX="402.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleCreateanAccount" prefHeight="38.0" prefWidth="142.0" text="Signup" />
            <Text layoutX="334.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Deliver Eats!" textAlignment="CENTER" wrappingWidth="245.12892150878906" />
            <Button fx:id="FXalreadyauserButton" layoutX="402.0" layoutY="326.0" mnemonicParsing="false" onAction="#handleAlreadyaUser" prefHeight="32.0" prefWidth="142.0" text="Already a user?" />
