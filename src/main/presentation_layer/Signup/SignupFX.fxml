  <children>
    <AnchorPane fx:id="FXsignupPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="FXsignupButton" layoutX="403.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleCreateanAccount" prefHeight="38.0" prefWidth="142.0" text="Customer Signup" />
            <Text layoutX="334.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Deliver Eats!" textAlignment="CENTER" wrappingWidth="245.12892150878906" />
            <Button fx:id="FXalreadyauserButton" layoutX="402.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleAlreadyaUser" prefHeight="32.0" prefWidth="142.0" text="Already a user?" />
            <Text layoutX="350.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please create an account below" textAlignment="CENTER" wrappingWidth="245.12892150878906" />
          <Button fx:id="createRestaurantBtn" layoutX="402.0" layoutY="304.0" mnemonicParsing="false" onAction="#handleCreateRestaurant" prefHeight="38.0" prefWidth="142.0" text="Create New Restaurant" />
            <ImageView fitHeight="492.0" fitWidth="465.0" layoutY="-56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/christine-siracusa-vzX2rgUbQXM-unsplash.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="FXusernameField" layoutX="398.0" layoutY="102.0" promptText="Email" />
            <TextField fx:id="FXAddressField" layoutX="399.0" layoutY="142.0" promptText="Address" />
            <PasswordField fx:id="FXpasswordField" layoutX="398.0" layoutY="178.0" promptText="Password" />
            <PasswordField fx:id="FXconfirmpasswordField" layoutX="398.0" layoutY="218.0" promptText="Confirm Password" />
            <Label fx:id="FXsignupmessageField" layoutX="399.0" layoutY="243.0" prefHeight="25.0" prefWidth="149.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
